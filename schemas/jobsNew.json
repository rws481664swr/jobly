{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 40
    },
    "salary": {
      "type": "integer"
    },
    "equity": {
      "type": "string",
      "minLength": 1,
      "maxLength": 5
    },
    "companyHandle": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    }
  },
  "additionalProperties": false,
  "required": [
    "title",
    "salary",
    "companyHandle",
    "equity"
  ]
}